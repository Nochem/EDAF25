package model;

import java.util.Map;
import java.util.TreeMap;

public class Sheet implements expr.Environment {

	private Map<String, Slot> map;
	private String lastCreatedSlot;
	private SlotFactory factory;

	public Sheet() {
		map = new TreeMap<String, Slot>();
		lastCreatedSlot=null;
		factory = new SlotFactory();
	}

	public void insertSlot(String slotAddress) {
		Slot newSlot=factory.newSlot(slotAddress); //Sheet beh√∂ver vidarebefodra undantag som kastas i factory till UI:t.
		map.put(slotAddress, newSlot);;
	}
	

	public double value(String str) {
		return map.get(str).getValue();
	}

}